<?xml version="1.0" encoding="UTF-8"?>

<?import com.jfoenix.controls.JFXButton?>
<?import com.jfoenix.controls.JFXTextField?>
<?import com.lynden.gmapsfx.GoogleMapView?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane id="AnchorPane" prefHeight="627.0" prefWidth="1070.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="testlogin.GestionCentreController">
   <children>
      <JFXTextField fx:id="tnom1" layoutX="148.0" layoutY="151.0" />
      <ImageView fitHeight="773.0" fitWidth="1070.0" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@image/Interface%20module.png" />
         </image>
      </ImageView>
      <Label layoutX="437.0" layoutY="84.0" text="Selectionnez le centre que vous voulez supprimer" textFill="#e42727">
         <font>
            <Font name="Times New Roman" size="19.0" />
         </font>
      </Label>
      <JFXTextField fx:id="nom_centre" layoutX="68.0" layoutY="176.0" opacity="0.44" text="nom" />
      <JFXTextField fx:id="adresse_centre" layoutX="68.0" layoutY="230.0" opacity="0.44" text="adresse" />
      <Label layoutX="46.0" layoutY="132.0" text="Voulez-vous ajouter un centre? ">
         <font>
            <Font name="Times New Roman" size="16.0" />
         </font>
      </Label>
      <JFXTextField fx:id="das_centre" layoutX="68.0" layoutY="284.0" opacity="0.44" text="domaine d'activités" />
      <JFXTextField fx:id="mail_centre" layoutX="68.0" layoutY="347.0" opacity="0.44" prefHeight="25.0" prefWidth="153.0" text="ex adressemail@gmail.com" />
      <JFXTextField fx:id="numero_centre" layoutX="68.0" layoutY="402.0" opacity="0.44" promptText="n° téléphone" />
      <JFXButton fx:id="ajouter_centre" layoutX="109.0" layoutY="513.0" onAction="#ajouterCentreAction" text="Ajouter">
         <font>
            <Font name="Times New Roman" size="16.0" />
         </font>
      </JFXButton>
      <JFXTextField fx:id="image_centre" layoutX="68.0" layoutY="460.0" opacity="0.44" promptText="n° téléphone" text="chemin absolu de votre image" />
      <GoogleMapView fx:id="mapView" layoutX="507.0" layoutY="432.0" prefHeight="173.0" prefWidth="477.0" />
      <JFXTextField fx:id="adresseto" layoutX="222.0" layoutY="230.0" onAction="#mapInitialized" />
      <JFXButton layoutX="260.0" layoutY="302.0" onAction="#toTextFieldAction" text="rechercher" />
      <ImageView fitHeight="25.0" fitWidth="25.0" layoutX="333.0" layoutY="551.0" onMouseClicked="#imprimerListeCentre" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@image/imprime.png" />
         </image>
      </ImageView>
      <ImageView fitHeight="31.0" fitWidth="42.0" layoutX="206.0" layoutY="457.0" onMouseClicked="#ajouterImageCentre" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@image/image.jpg" />
         </image>
      </ImageView>
      <Label layoutX="46.0" layoutY="554.0" text="Voulez-vous imprimez votre liste de centre?">
         <font>
            <Font name="Times New Roman" size="16.0" />
         </font>
      </Label>
      <TableView fx:id="table" layoutX="391.0" layoutY="119.0" prefHeight="320.0" prefWidth="632.0">
        <columns>
          <TableColumn fx:id="col_idcentre" prefWidth="75.0" text="ID" />
          <TableColumn fx:id="col_nomcentre" onEditCommit="#modifierNomCentre" prefWidth="75.0" text="Nom" />
            <TableColumn fx:id="col_adressecentre" onEditCommit="#modifierAdresseCentre" prefWidth="75.0" text="Adresse" />
            <TableColumn fx:id="col_dascentre" onEditCommit="#modifierDasCentre" prefWidth="75.0" text="DAS" />
            <TableColumn fx:id="col_mailcentre" onEditCommit="#modifierMailCentre" prefWidth="75.0" text="mail" />
            <TableColumn fx:id="col_telephonecentre" onEditCommit="#modifierTelephoneCentre" prefWidth="75.0" text="telephone" />
            <TableColumn fx:id="col_imagecentre" onEditCommit="#modifierImageCentre" prefWidth="126.0" text="image" />
        </columns>
      </TableView>
      <JFXButton layoutX="835.0" layoutY="82.0" onAction="#supprimerCentre" text="Supprimer">
         <font>
            <Font name="Times New Roman" size="12.0" />
         </font>
      </JFXButton>
   </children>
</AnchorPane>
